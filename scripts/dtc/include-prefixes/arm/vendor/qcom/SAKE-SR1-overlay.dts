/*    ====================================
 *    overlay for lahaina-mtp.dtsi - start
 *    ====================================
 */
 
&qupv3_se15_i2c{
	status = "ok";
	qcom,clk-freq-out = <400000>;

	nq@28 {
		compatible = "nxp,pn544";
		reg = <0x28>;
		nxp,pn544-irq = <&tlmm 87 0x00>;
		nxp,pn544-ven = <&tlmm 3 0x00>;
		nxp,pn544-fw-dwnld = <&tlmm 86 0x00>;
		qcom,sn-clkreq = <&tlmm 63 0x00>;
		nxp,pn544-ese-pwr = <&tlmm 47 0x00>;
		interrupt-parent = <&tlmm>;
		interrupts = <87 0>;
		interrupt-names = "nfc_irq";
		pinctrl-names = "nfc_active", "nfc_suspend";
		pinctrl-0 = <&nfc_int_active &nfc_enable_active
				&nfc_clk_req_active &nfc_fwdl_active>;
		pinctrl-1 = <&nfc_int_suspend &nfc_enable_suspend
				&nfc_clk_req_suspend &nfc_fwdl_suspend>;
	};
};

/* NFC eSE */
&qupv3_se14_spi {
	status = "ok";
	p61@0 {
		compatible = "nxp,p61";
		reg = <0x0>;
		spi-max-frequency = <20000000>;
	};
};

&extcon_usb1 {
	status = "disable";
};

/* ASUS_BSP +++ icm20690 */
&qupv3_se19_i2c {
        status = "ok";
        icm206xx_i2c@68 {
                compatible = "invn,icm20690-i2c";
                reg = <0x68>;
                invn,gpio-int = <&tlmm 7 1>;
                invn,use-interrupt;
                invn,place = "Portrait Down Back Side";
                icm206xx-supply = <&pm8008j_l7>;
                icm206xx-vdd-supply = <&pm8008j_l5>;
                pinctrl-names = "cam_default", "cam_suspend";
                pinctrl-0 = <&cam_icm_active>;
                pinctrl-1 = <&cam_icm_suspend>;
        };
};
/* ASUS_BSP --- icm20690 */

&qupv3_se0_i2c {
	Semtech_sx932x@28 {
		compatible = "Semtech,sx932x";
		reg = <0x28>;
		//interrupt-parent = <&msm_gpio>;
		//interrupts = <34 0>;
		pinctrl-names = "sx932x_active";
		pinctrl-0 = <&sx932x_active>;
		Semtech,nirq-gpio = <&tlmm 91 0x00>;
		status = "ok";	/**/
		Semtech,reg-num = <46>; /* total registers number to initialize*/
        Semtech,reg-init  = /bits/ 8 < 0x10 0x16
                                0x20 0x00
                                0x23 0x00
                                0x24 0x44
                                0x26 0x01
                                0x27 0x44
                                0x28 0x01 /*PH0 to CS0 input*/
                                0x29 0x10
                                0x2A 0x19 /*PH2 CS2-input,CS1-dync,CS0-HZ*/
                                0x2B 0x00
                                0x2C 0x12
                                0x2D 0x08
                                0x30 0x09
                                0x31 0x09
                                0x32 0x08
                                0x33 0x20
                                0x34 0x0C
                                0x35 0x00
                                0x36 0x1B
                                0x37 0x1B
                                0x40 0x00
                                0x41 0x00
                                0x42 0x00
                                0x43 0x00
                                0x44 0x00
                                0x45 0x05
                                0x46 0x00
                                0x47 0x00
                                0x48 0x00
                                0x49 0x80
                                0x4A 0x00
                                0x4B 0x00
                                0x4C 0x00
                                0x4D 0x00
                                0x4E 0x80
                                0x4F 0x0C
                                0x50 0x00
                                0x51 0x70
                                0x52 0x20
                                0x53 0xE1
                                0x54 0xF0
                                0x05 0x70
                                0x06 0x00
                                0x07 0x00
                                0x08 0x00
								0x11 0x24>;

	};
		
};

