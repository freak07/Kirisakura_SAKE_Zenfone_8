/*    ====================================
 *    overlay for lahaina-pinctrl.dtsi - start
 *    ====================================
 */

&tlmm {
	/* ASUS_BSP +++ icm20690 */
	cam_icm_active: cam_icm_active {
		mux {
			pins = "gpio124";
			function = "gpio";
		};

		config {
			pins = "gpio124";
			bias-pull-up;
			drive-strength = <8>;
		};
	};

	cam_icm_suspend: cam_icm_suspend {
		mux {
			pins = "gpio124";
			function = "gpio";
		};

		config {
			pins = "gpio124";
			bias-pull-down;
			drive-strength = <2>; 
		};
	};
	/* ASUS_BSP --- icm20690 */
};

&tlmm{
	sx932x_active {
		sx932x_active: sx932x_active {
			mux {
				pins = "gpio91";
				function = "gpio";
			};
			config {
				pins = "gpio91";
				bias-pull-up;
				drive-strength = <6>;
			};
		};
	};
};

