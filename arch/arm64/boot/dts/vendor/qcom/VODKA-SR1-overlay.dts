&usb2_phy0 {
	qcom,param-override-seq = <0x67 0x6c	/* override_x0 */
				   0x0c 0x70	/* override_x1 */
				   0x17 0x74	/* override_x2 */
				   0x03 0x78>;	/* override_x3 */
};

//disable USB1 SSPHY +++
&usb0 {
	dwc3@a600000 {
		usb-phy = <&usb2_phy0>, <&usb_nop_phy>;
		maximum-speed = "high-speed";
	};
};

&usb_qmp_dp_phy {
	status = "disabled";
};
//disable USB1 SSPHY ---

//disable USB2 +++
&usb1 {
	status = "disabled";
};

&usb2_phy1 {
	status = "disabled";
};

&usb_qmp_phy {
	status = "disabled";
};
