/*    ====================================
 *    overlay for lahaina-pinctrl.dtsi - start
 *    ====================================
 */

&tlmm{
		//Vodka Latch_sensor +++
		latch_int_pin {
			gpio_latch_active: gpio_latch_active {
				mux {
					pins = "gpio79";
					function = "gpio";
				};

				config {
					pins = "gpio79";
					drive-strength = <2>;
					bias-pull-up;
				};
			};
			gpio_latch_suspend: gpio_latch_suspend {
				mux {
					pins = "gpio79";
					function = "gpio";
				};

				config {
					pins = "gpio79";
					drive-strength = <2>;
					bias-no-pull;
				};
			};
		};
		//Vodka Latch_sensor ---

		//Vodka Hall_sensor +++
		hall_int_pin {

			gpio_hall_active: gpio_hall_active {
				mux {
					pins = "gpio50";
					function = "gpio";
				};

				config {
					pins = "gpio50";
					drive-strength = <2>;
					bias-pull-down;
				};
			};
			gpio_hall_suspend: gpio_hall_suspend {
				mux {
					pins = "gpio50";
					function = "gpio";
				};

				config {
					pins = "gpio50";
					drive-strength = <2>;
					bias-pull-down;
				};
			};
		};
		//Vodka Hall_sensor ---
};

&tlmm{
	sx932x_active {
		sx932x_active: sx932x_active {
			mux {
				pins = "gpio91";
				function = "gpio";
			};
			config {
				pins = "gpio91";
				bias-pull-up;
				drive-strength = <6>;
			};
		};
	};
};

